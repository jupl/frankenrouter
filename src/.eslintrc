{
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "arrow-spacing": [2, {
      "before": true,
      "after": true
    }],
    "constructor-super": 2,
    "generator-star-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "require-yield": 2
  }
}
